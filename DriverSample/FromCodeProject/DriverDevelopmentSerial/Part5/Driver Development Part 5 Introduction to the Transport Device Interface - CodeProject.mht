    UINT NumberOfSeconds =3D 60*3;=0A=
    TDI_COMPLETION_CONTEXT TdiCompletionContext;=0A=
=0A=
    KeInitializeEvent(&amp;TdiCompletionContext.kCompleteEvent, =0A=
                                    NotificationEvent, FALSE);=0A=
=0A=
    <SPAN class=3D"code-comment">/*</SPAN><SPAN class=3D"code-comment">=0A=
     * The TDI Device Object is required to send =0A=
     *                        these requests to the TDI Driver.=0A=
     */</SPAN>=0A=
=0A=
    pTdiDevice =3D IoGetRelatedDeviceObject(pfoConnection);=0A=
    =0A=
    <SPAN class=3D"code-comment">/*</SPAN><SPAN class=3D"code-comment">=0A=
     * Step 1: Build the IRP.  TDI defines several macros and functions =0A=
     *         that can quickly create IRP's, etc. for variuos purposes. =
 =0A=
     *         While this can be done manually it's easiest to use the =
macros.=0A=
     *=0A=
     */</SPAN>=0A=
    pIrp =3D TdiBuildInternalDeviceControlIrp(TDI_DISCONNECT, =
pTdiDevice, =0A=
                  pfoConnection, =
&amp;TdiCompletionContext.kCompleteEvent,=0A=
                  &amp;IoStatusBlock);=0A=
=0A=
    <SPAN class=3D"code-keyword">if</SPAN>(pIrp)=0A=
    {=0A=
        <SPAN class=3D"code-comment">/*</SPAN><SPAN =
class=3D"code-comment">=0A=
         * Step 2: Add the correct parameters into the IRP.=0A=
         */</SPAN>=0A=
=0A=
        <SPAN class=3D"code-comment">/*</SPAN><SPAN =
class=3D"code-comment">=0A=
         *  Time out value=0A=
         */</SPAN>=0A=
=0A=
        TimeOut.QuadPart =3D 10000000L;=0A=
        TimeOut.QuadPart *=3D NumberOfSeconds;=0A=
        TimeOut.QuadPart =3D -(TimeOut.QuadPart);=0A=
=0A=
=0A=
        TdiBuildDisconnect(pIrp, pTdiDevice, pfoConnection, NULL, NULL, =0A=
                            &amp;TimeOut, TDI_DISCONNECT_ABORT, NULL, =0A=
                            &amp;ReturnConnectionInfo);=0A=
=0A=
        NtStatus =3D IoCallDriver(pTdiDevice, pIrp);=0A=
=0A=
        <SPAN class=3D"code-comment">/*</SPAN><SPAN =
class=3D"code-comment">=0A=
         *  If the status returned is STATUS_PENDING this means that the =0A=
         *  IRP will not be completed synchronously and the driver has =0A=
